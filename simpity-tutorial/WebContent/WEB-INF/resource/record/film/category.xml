<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<record xmlns="http://com.exilant.exility/schema"
	name="category"
	keyToBeGenerated="true" 
	recordType="storage" 
	tableName="category"
	description="film category"
	defaultGridName="categories"
	>
	<fields>
		<field name="categoryId"
			columnName="category_id" 
			columnType="primaryKey" 
			label="categoryId"
			description="internally generated key"
			dataType="id"
			defaultInputType="hiddenField"  >
			<pageFieldAttributes />
		</field>
		<field name="name"
			columnName="name" 
			columnType="data" 
			dataType="shortName"
			label="Category" 
			description=" category Name"
			defaultInputType="textInputField" 
			>
			<pageFieldAttributes />
		</field>
		<field name="lastUpdate"
			columnName="last_update" 
			columnType="modifiedTimeStamp" 
			dataType="timeStamp"
			defaultInputType="hiddenField" 
			label="lastUpdate" 
			description="timestamp"
			>
			<pageFieldAttributes />
		</field>
	</fields>
</record>